<!DOCTYPE html>
<html lang="pt-br" ng-app="app">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Portifolio</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Portifolio.styles.css" asp-append-version="true" />
</head>
<body>
    <header class="menu">
        <div class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white mb-3">
            <div class="container-fluid container py-4">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Portifolio</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" href="#conteudo1">Botão 1</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" href="#conteudo2">Botão 2</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" href="#conteudo3">Botão 3</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" href="#conteudo4">Botão 4</a>
                        </li>
                        
                        <li class="nav-item">
                            <a class="nav-link text-dark" href="#section-primeiro">primeiro</a>
                        </li>
                        
                        <li class="nav-item">
                            <a class="nav-link text-dark" href="#section-meio">meio</a>
                        </li>
                        
                        <li class="nav-item">
                            <a class="nav-link text-dark" href="#section-ultimo">ultimo</a>
                        </li>

                    </ul>
                </div>
            </div>
        </div>
    </header>
    <div>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - Portifolio - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/angular/angular.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
